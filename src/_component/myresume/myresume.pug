.myresume
  .myresume-wrapper
    .myresume-title #{resume.myresume.title}
  each component in resume.myresume.component
    .myresume-component
      .myresume-component-title #{component.title}
      if component.type === 'list'
        each list in component.list
          .myresume-component-list
            each listItem in list
              .myresume-list-item #{listItem}
